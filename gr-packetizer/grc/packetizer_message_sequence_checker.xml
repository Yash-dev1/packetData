<?xml version="1.0"?>
<block>
  <name>Message sequence checker</name>
  <key>packetizer_message_sequence_checker</key>
  <category>[Packetizer]</category>
  <import>import packetizer</import>
  <make>packetizer.message_sequence_checker($num_key)</make>

  <param>
    <name>Field to check</name>
    <key>num_key</key>
    <value>"packet_num"</value>
    <type>string</type>
  </param>


 <param>
    <name>Number of bits of sequence field (0 if unknown)</name>
    <key>nb_bits</key>
    <value>0</value>
    <type>int</type>
  </param>

  <sink>
    <name>data</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
</block>
